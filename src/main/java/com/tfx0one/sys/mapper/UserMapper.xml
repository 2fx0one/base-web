<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tfx0one.sys.mapper.UserMapper">

    <select id="getByLoginName" resultType="com.tfx0one.sys.entity.User">
        select * from sys_user where sys_user.login_name = #{loginName}
    </select>
</mapper>
